# Google Maps API Configuration
# Copy this file to .env and add your actual API key
# NEVER commit the .env file to version control

GOOGLE_API_KEY=your_api_key_here

# CORS Configuration
# Set to your actual domain, or * for all (not recommended)
ALLOWED_ORIGIN=https://yourdomain.com

# Referer Validation (Extra Protection)
# Only allow requests from this domain
ALLOWED_REFERER=https://yourdomain.com
# Require referer header (true/false) - set to true for strict security
REQUIRE_REFERER=false

# Rate Limiting
# Maximum requests per IP per hour
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_HOURS=1
